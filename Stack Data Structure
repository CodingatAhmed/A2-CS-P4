# Pop Operation
myArr = [1,3,5,7,8]
StackPoint = len(myArr) - 1
def Pop():
    global StackPoint
    if StackPoint > -1:
        myArr[StackPoint] = None
        StackPoint -= 1
        print(myArr)
    else:
        print("Stack is empty,cannot pop")
